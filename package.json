{
  "name": "gourmet-sentence-pairs-evaluation",
  "version": "1.0.0",
  "description": "Description",
  "author": "susiecoleman",
  "license": "UNLICENSED",
  "main": "./dist/server.js",
  "scripts": {
    "start": "node .",
    "build": "tsc",
    "lint": "tslint --project tsconfig.json",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "spec": "speculate --release ${BUILD_NUMBER}",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch",
    "pre-commit-tests": "yarn jest --bail --changedSince=master",
    "valid": "yarn lint && yarn build && yarn test"
  },
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "aws-sdk": "^2.513.0",
    "csv-writer": "^1.5.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "hbs": "^4.0.4",
    "multer": "^1.4.2",
    "typescript": "^3.5.3",
    "underscore": "^1.9.2",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "winston-cloudwatch": "^2.0.9"
  },
  "devDependencies": {
    "@shelf/jest-dynamodb": "^1.6.0",
    "@types/archiver": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.18",
    "@types/multer": "^1.3.9",
    "@types/node": "^12.7.2",
    "@types/supertest": "^2.0.8",
    "@types/underscore": "^1.9.4",
    "@types/uuid": "^3.4.5",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "speculate": "^2.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.40",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "preset": "@shelf/jest-dynamodb"
  },
  "spec": {
    "requires": [
      "cloud-httpd24-ssl-services-devs-staff"
    ],
    "executable": [
      "./bake-scripts/*"
    ],
    "post": [
      "mkdir -p /etc/bake-scripts/gourmet-sentence-pairs-evaluation",
      "cp -rf /usr/lib/gourmet-sentence-pairs-evaluation/bake-scripts/* /etc/bake-scripts/gourmet-sentence-pairs-evaluation/"
    ]
  }
}
